  <rdf:Description rdf:about="http://data.europa.eu/esco/occupation/898f99f7-8c0a-4212-9e58-9ab00379e5ae">
    <rdf:type rdf:resource="http://data.europa.eu/esco/model#MemberConcept"/>
    <rdfs:label xml:lang="en">fitter and turner</rdfs:label>
    <dcterms:description xml:lang="en">Fitters and turners use machine tools to create and modify metal parts according to set specifications in order to fit components for machinery. They ensure the finished components are ready for assembly.</dcterms:description>
    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7223</skos:notation>
  </rdf:Description>

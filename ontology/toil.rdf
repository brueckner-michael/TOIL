  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/esco/occupation/336cf331-8082-4a0c-bf5f-e7c54706211c">
    <rdf:type rdf:resource="http://data.europa.eu/esco/model#MemberConcept"/>
    <rdfs:label xml:lang="en">bicycle mechanic</rdfs:label>
    <dcterms:description xml:lang="en">Bicycle mechanics maintain and repair a variety of bicycle models and component parts. They may perform customized alterations, according to their client's preferences.</dcterms:description>
    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7234</skos:notation>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/esco/occupation/173a882d-0a14-4919-ac1b-23e8ee8273de">
    <rdf:type rdf:resource="http://data.europa.eu/esco/model#MemberConcept"/>
    <rdfs:label xml:lang="en">import export manager in hardware, plumbing and heating equipment and supplies</rdfs:label>
    <dcterms:description xml:lang="en">Import export managers in hardware, plumbing and heating equipment and supplies install and maintain procedures for cross-border business, coordinating internal and external parties.</dcterms:description>
    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1324</skos:notation>
  </rdf:Description>

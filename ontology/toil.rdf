  <rdf:Description rdf:about="http://data.europa.eu/esco/occupation/7c320679-131d-4828-87ed-94886614b7ed">
    <rdf:type rdf:resource="http://data.europa.eu/esco/model#MemberConcept"/>
    <rdfs:label xml:lang="en">pump operator</rdfs:label>
    <dcterms:description xml:lang="en">Pump operators assist firefighting operations by controlling the pumps which supply water and other substances for extinguishing fires. They ensure the substance is delivered in the right amount and under the correct  pressure through the firehose.</dcterms:description>
    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5411</skos:notation>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/esco/occupation/4cdfac8c-b000-4279-985e-31bc1fbe2447">
    <rdf:type rdf:resource="http://data.europa.eu/esco/model#MemberConcept"/>
    <rdfs:label xml:lang="en">underground miner</rdfs:label>
    <dcterms:description xml:lang="en">Underground miners perform a wide range of ancillary underground mining operations such as inspections, conveyor attendance and the transport of equipment and consumable materials from the surface to the point of extraction underground.</dcterms:description>
    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8111</skos:notation>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/esco/occupation/42381741-f3a7-4b26-b35c-da6fad9f1cfd">
    <rdf:type rdf:resource="http://data.europa.eu/esco/model#MemberConcept"/>
    <rdfs:label xml:lang="en">product development engineering technician</rdfs:label>
    <dcterms:description xml:lang="en">Product development engineering technicians improve efficiency of product development, set up equipment and develop and test solutions to solve technical problems. They work closely with engineers and technologists, inspect products, conduct tests and collect data.</dcterms:description>
    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3119</skos:notation>
  </rdf:Description>
